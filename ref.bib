title = {{A data-motivated density-dependent diffusion model of in vitro glioblastoma growth.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26775861},
volume = {12},
year = {2015}
}
@article{Stepien2018,
abstract = {Glioblastoma multiforme is a deadly brain cancer in which tumor cells excessively proliferate and migrate. The first mathematical models of the spread of gliomas featured reaction-diffusion equations, and later an idea emerged through experimental study called the “Go or Grow” hypothesis in which glioma cells have a dichotomous behavior: a cell either primarily proliferates or primarily migrates. We analytically investigate an extreme form of the “Go or Grow” hypothesis where tumor cell motility and cell proliferation are considered as separate processes. Different solution types are examined via approximate solution of traveling wave equations, and we determine conditions for various wave front forms.},
author = {Stepien, Tracy L. and Rutter, Erica M. and Kuang, Yang},
doi = {10.1137/17M1146257},
