month = {jan},
number = {3},
pages = {1778--1801},
publisher = {Society for Industrial and Applied Mathematics},
title = {{Traveling Waves of a Go-or-Grow Model of Glioma Growth}},
url = {https://epubs.siam.org/doi/10.1137/17M1146257},
volume = {78},
year = {2018}
}
@article{Stupp2005,
