publisher = { American Society of Clinical Oncology},
title = {{Dose-dense temozolomide for newly diagnosed glioblastoma: a randomized phase III clinical trial.}},
url = {http://ascopubs.org/doi/10.1200/JCO.2013.49.6968 http://www.ncbi.nlm.nih.gov/pubmed/24101040 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3816958},
